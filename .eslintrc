{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
},
  "rules": {
    "max-len": ["error", 150, 2, {
      "ignoreUrls": true,
      "ignoreComments": false,
      "ignoreRegExpLiterals": true,
      "ignoreStrings": true,
      "ignoreTemplateLiterals": true,
      "code": 150,
      "tabWidth": 4,
      "ignoreTrailingComments": false
    }]
  }
}
